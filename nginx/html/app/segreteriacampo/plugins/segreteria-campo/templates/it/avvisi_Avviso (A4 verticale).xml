<?xml version='1.0' encoding='iso-8859-1'?>

<pdf creator="https://github.com/linkingtechnologies/protezionet-segreteria-campo" title="Avviso">
<body format='A4' unit='mm' margins='15'>

<!-- 85x54 -->
<!-- $BeginBlock form -->
<page>

	<image top="15" left="20" height="15" width="15" position="absolute" file="<!-- $Include var/templates/it/logo.txt -->" />
	<paragraph top="15" left="38" fontsize="12" fontstyle="B" position="absolute" border="0" width="150" textalign="L"><!-- $Include var/templates/it/evento.txt --></paragraph>
	<paragraph top="20" left="38" fontsize="12" fontstyle="B" position="absolute" border="0" width="150" textalign="L"><!-- $Include var/templates/it/comune.txt --></paragraph>
	<paragraph top="25" left="38" fontcolor="red" fontsize="12" fontstyle="B" position="absolute" border="0" width="150" textalign="L"><!-- $Include var/templates/it/segreteriacampo.txt --> <!-- $Include var/templates/it/nomecampo.txt --></paragraph>
	<paragraph position="absolute" top="60" left="20" width="170" font="${carattere}" fontsize="${dimensione titolo}" fontcolor="black" border="0" textalign="C">${oggetto}</paragraph>
	<paragraph position="absolute" top="80" left="20" width="170" font="${carattere}" fontsize="${dimensione testo}" fontcolor="black" linespacing="150" border="0" textalign="C" lineheight="fontsize">${testo}</paragraph>
	<barcode x="135" y="270" height="7" width=".2" position="absolute" barcode="${codice riga}" norm="code39extend"/>
	<paragraph position="absolute" top="270" left="20" width="170" font="${carattere}" fontsize="12" fontcolor="black" linespacing="150" border="0" textalign="L">${data}</paragraph>
</page>

<!-- $EndBlock form -->

<!-- $BeginBlock table -->
<page fontsize="10">
<!-- $BeginBlock row1 -->

	<image top="15" left="20" height="15" width="15" position="absolute" file="<!-- $Include var/templates/it/logo.txt -->" />
	<paragraph top="15" left="38" fontsize="12" fontstyle="B" position="absolute" border="0" width="150" textalign="L"><!-- $Include var/templates/it/evento.txt --></paragraph>
	<paragraph top="20" left="38" fontsize="12" fontstyle="B" position="absolute" border="0" width="150" textalign="L"><!-- $Include var/templates/it/comune.txt --></paragraph>
	<paragraph top="25" left="38" fontcolor="red" fontsize="12" fontstyle="B" position="absolute" border="0" width="150" textalign="L"><!-- $Include var/templates/it/segreteriacampo.txt --> <!-- $Include var/templates/it/nomecampo.txt --></paragraph>
	<paragraph position="absolute" top="60" left="20" width="170" font="${carattere}" fontsize="${dimensione titolo}" fontcolor="black" border="0" textalign="C">${oggetto}</paragraph>
	<paragraph position="absolute" top="80" left="20" width="170" font="${carattere}" fontsize="${dimensione testo}" fontcolor="black" linespacing="150" border="0" textalign="C" lineheight="fontsize">${testo}</paragraph>
	<paragraph position="absolute" top="270" left="20" width="170" font="${carattere}" fontsize="12" fontcolor="black" linespacing="150" border="0" textalign="L">${data}</paragraph>
	<barcode x="135" y="270" height="7" width=".2" position="absolute" barcode="${cod. riga}" norm="code39extend"/>

<!-- $EndBlock row1 -->

</page>
<!-- $EndBlock table-->

</body>
</pdf>
